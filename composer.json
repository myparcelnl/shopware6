{
  "name": "mypashopware/myparcel",
  "description": "MyParcel integration for Shopware 6",
  "version": "1.0.0",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "MyParcel",
      "homepage": "https://www.myparcel.nl",
      "role": "Manufacturer"
    },
      {
          "name": "Kiener Digital Commerce",
          "homepage": "https://www.kiener.nl",
          "role": "Developer"
      }
  ],
  "require": {
    "shopware/core": "*",
    "shopware/storefront": "*",
    "shopware/administration": "*",
    "myparcelnl/sdk": "^5.0"
  },
  "extra": {
    "shopware-plugin-class": "MyPaShopware\\MyParcel",
    "copyright": "(c) by MyParcel",
    "label": {
      "en-GB": "MyParcel integration for Shopware 6",
      "nl-NL": "MyParcel integratie voor Shopware 6"
    },
    "description": {
      "en-GB": "MyParcel integration for Shopware 6",
      "nl-NL": "MyParcel integratie voor Shopware 6"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "MyPaShopware\\": "src/",
      "MyParcelNL\\Sdk\\src\\": "vendor/myparcelnl/sdk/src"
    }
  }
}
